    "postcss": "^8.4.32",
    "preline": "^2.4.1",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "dev": "node ./node_modules/tailwindcss/lib/cli.js -i ./assets/css/tailwind.css -o ./assets/dist/main.css --postcss --watch",
